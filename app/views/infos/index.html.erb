<p id="notice"><%= notice %></p>

<h1>Listing Infos</h1>

<table>
  <thead>
    <tr>
      <th>Birthday</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Email</th>
      <th>Description</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @infos.each do |info| %>
      <tr>
        <td><%= info.birthday %></td>
        <td><%= info.address %></td>
        <td><%= info.latitude %></td>
        <td><%= info.longitude %></td>
        <td><%= info.email %></td>
        <td><%= info.description %></td>
        <td><%= info.user_id %></td>
        <td><%= link_to 'Show', info %></td>
        <td><%= link_to 'Edit', edit_info_path(info) %></td>
        <td><%= link_to 'Destroy', info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info', new_info_path %>
